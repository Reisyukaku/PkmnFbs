struct Vec3f {
    X:float;
    Y:float;
    Z:float;
}

table TransformTbl {
    Vec1:Vec3f;
    Vec2:Vec3f;
    Vec3:Vec3f;
}

table Effect {
    Name:string;
    Transform:TransformTbl;
    unk2:Vec3f;
    unk3:Vec3f;
    unk4:uint;
    unk5:uint;
    ParentBone:string;
}

table SkeletalLocator {
    unk:uint;
    Effects:[Effect];
    unk1:uint;
    unk2:uint;
}

root_type SkeletalLocator;